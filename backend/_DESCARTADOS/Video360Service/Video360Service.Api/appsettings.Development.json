{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=video360service;Username=postgres;Password=postgres"
  },
  "Jwt": {
    "Key": "super-secret-key-for-development-only-32chars",
    "Issuer": "okla",
    "Audience": "okla"
  },
  "Seq": {
    "ServerUrl": "http://localhost:5341"
  },
  "Providers": {
    "FfmpegApi": {
      "BaseUrl": "https://api.ffmpeg-api.com",
      "IsEnabled": true,
      "TimeoutSeconds": 120,
      "MaxRetries": 3,
      "CostPerVideoUsd": 0.011
    },
    "ApyHub": {
      "BaseUrl": "https://api.apyhub.com",
      "IsEnabled": true,
      "TimeoutSeconds": 120,
      "MaxRetries": 3,
      "CostPerVideoUsd": 0.009
    },
    "Cloudinary": {
      "BaseUrl": "https://api.cloudinary.com/v1_1",
      "IsEnabled": true,
      "TimeoutSeconds": 180,
      "MaxRetries": 3,
      "CostPerVideoUsd": 0.012
    },
    "Imgix": {
      "BaseUrl": "https://api.imgix.com",
      "SourceDomain": "your-source.imgix.net",
      "IsEnabled": false,
      "TimeoutSeconds": 120,
      "MaxRetries": 3,
      "CostPerVideoUsd": 0.018
    },
    "Shotstack": {
      "BaseUrl": "https://api.shotstack.io/stage",
      "IsEnabled": false,
      "TimeoutSeconds": 300,
      "MaxRetries": 2,
      "CostPerVideoUsd": 0.05
    }
  },
  "Storage": {
    "S3": {
      "BucketName": "okla-video360-dev",
      "Region": "us-east-1",
      "ServiceUrl": "http://localhost:4566",
      "ForcePathStyle": true,
      "CdnBaseUrl": "http://localhost:4566/okla-video360-dev"
    }
  },
  "Secrets": {
    "FfmpegApi": {
      "ApiKey": "your-ffmpeg-api-key"
    },
    "ApyHub": {
      "ApiToken": "your-apyhub-token"
    },
    "Cloudinary": {
      "CloudName": "your-cloud-name",
      "ApiKey": "your-cloudinary-key",
      "ApiSecret": "your-cloudinary-secret"
    },
    "Imgix": {
      "ApiKey": "your-imgix-key",
      "SecureUrlToken": "your-secure-token"
    },
    "Shotstack": {
      "ApiKey": "your-shotstack-key"
    },
    "S3": {
      "AccessKey": "test",
      "SecretKey": "test"
    }
  }
}
