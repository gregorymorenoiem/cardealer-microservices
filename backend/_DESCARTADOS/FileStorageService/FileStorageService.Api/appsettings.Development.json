{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "Storage": {
    "ProviderType": "Local",
    "BasePath": "./uploads",
    "EnableVirusScan": false,
    "EnableMetadataExtraction": true,
    "EnableImageOptimization": true
  }
}
