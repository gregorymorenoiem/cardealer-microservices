{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "EventTrackingService": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ClickHouse": "Host=clickhouse;Port=8123;Database=okla_events;User=default;Password=;Compression=true"
  },
  "EventTracking": {
    "BatchSize": 100,
    "FlushIntervalSeconds": 5,
    "RetentionDays": 90,
    "ArchiveAfterDays": 30
  }
}
