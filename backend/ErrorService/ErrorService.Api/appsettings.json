{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=errorservice;Username=postgres;Password=password"
  },
  "RateLimiting": {
    "Enabled": true,
    "MaxRequests": 100,
    "WindowSeconds": 60,
    "EnableLogging": true,
    "WhitelistedIps": [
      "127.0.0.1",
      "::1"
    ]
  },
  "ElasticSearch": {
    "ConnectionString": "http://localhost:9200",
    "DefaultIndex": "errorlogs",
    "Enable": false
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": "5672",
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "cardealer.events"
  },
  "ErrorService": {
    "QueueName": "error-queue",
    "ExchangeName": "error-exchange",
    "RoutingKey": "error.routing.key"
  }
}