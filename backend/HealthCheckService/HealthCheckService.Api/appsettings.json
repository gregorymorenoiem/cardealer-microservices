{
  "Services": {
    "ErrorService": "http://errorservice",
    "AuthService": "http://authservice",
    "NotificationService": "http://notificationservice",
    "SchedulerService": "http://schedulerservice",
    "AuditService": "http://auditservice",
    "VehicleService": "http://vehicleservice",
    "MediaService": "http://mediaservice",
    "ContactService": "http://contactservice"
  },
  "HealthCheck": {
    "CheckIntervalSeconds": 30,
    "TimeoutSeconds": 10
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "HealthCheckService": "Debug"
    }
  },
  "AllowedHosts": "*"
}
