{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "CacheConfiguration": {
    "DefaultTtlSeconds": 3600,
    "MaxTtlSeconds": 86400,
    "MaxKeySizeBytes": 1024,
    "MaxValueSizeBytes": 1048576,
    "EnableStatistics": true,
    "EnableCompression": false,
    "ConnectTimeoutMs": 5000,
    "SyncTimeoutMs": 5000,
    "RetryMaxAttempts": 3
  }
}
