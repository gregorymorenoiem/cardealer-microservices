{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "CacheConfiguration": {
    "DefaultTtlSeconds": 3600,
    "MaxTtlSeconds": 86400,
    "MaxKeySizeBytes": 1024,
    "MaxValueSizeBytes": 1048576,
    "EnableStatistics": true,
    "EnableCompression": false,
    "ConnectTimeoutMs": 5000,
    "SyncTimeoutMs": 5000,
    "RetryMaxAttempts": 3
  },
  "Service": {
    "Name": "CacheService",
    "Host": "localhost",
    "Port": "5004",
    "HealthCheckUrl": "http://localhost:5004/health"
  },
  "Consul": {
    "Address": "http://localhost:8500"
  }
}
