{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Seq": {
    "Url": "http://localhost:5341"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Alerting": {
    "EvaluationIntervalMinutes": 5
  },
  "Service": {
    "Name": "LoggingService",
    "Host": "localhost",
    "Port": "5005",
    "HealthCheckUrl": "http://localhost:5005/health"
  },
  "Consul": {
    "Address": "http://localhost:8500"
  }
}
