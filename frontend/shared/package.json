{
  "name": "@cardealer/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    },
    "./types": {
      "import": "./src/types/index.ts",
      "types": "./src/types/index.ts"
    },
    "./hooks": {
      "import": "./src/hooks/index.ts",
      "types": "./src/hooks/index.ts"
    },
    "./constants": {
      "import": "./src/constants/index.ts",
      "types": "./src/constants/index.ts"
    }
  },
  "scripts": {
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "typescript": "^5.6.3"
  }
}
