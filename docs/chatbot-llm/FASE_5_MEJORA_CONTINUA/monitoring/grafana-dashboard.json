      "targets": [
        {
          "expr": "okla_llm_requests_success_total / okla_llm_requests_total",
          "legendFormat": "Success Rate"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [{ "color": "green", "value": null }]
          }
