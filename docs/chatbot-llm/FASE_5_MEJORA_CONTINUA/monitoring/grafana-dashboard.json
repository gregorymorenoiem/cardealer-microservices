      "options": { "tooltip": { "mode": "multi" } },
      "title": "Response Latency (ms)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(okla_llm_response_duration_ms_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.90, rate(okla_llm_response_duration_ms_bucket[5m]))",
          "legendFormat": "p90"
        },
        {
          "expr": "histogram_quantile(0.95, rate(okla_llm_response_duration_ms_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(okla_llm_response_duration_ms_bucket[5m]))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 11,
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(okla_llm_requests_total[5m])",
          "legendFormat": "Total"
        },
        {
          "expr": "rate(okla_llm_requests_success_total[5m])",
          "legendFormat": "Success"
        },
        {
          "expr": "rate(okla_llm_requests_error_total[5m])",
          "legendFormat": "Error"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "id": 12,
      "title": "Tokens per Request",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(okla_llm_tokens_total[5m]) / rate(okla_llm_requests_success_total[5m])",
          "legendFormat": "Avg Tokens/Request"
        },
        {
          "expr": "rate(okla_llm_prompt_tokens_total[5m]) / rate(okla_llm_requests_success_total[5m])",
          "legendFormat": "Prompt Tokens"
        },
        {
          "expr": "rate(okla_llm_completion_tokens_total[5m]) / rate(okla_llm_requests_success_total[5m])",
          "legendFormat": "Completion Tokens"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": { "unit": "short" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "id": 13,
      "title": "Tokens per Second (Throughput)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(okla_llm_tokens_total[5m])",
          "legendFormat": "Total Tokens/s"
        },
        {
          "expr": "rate(okla_llm_completion_tokens_total[5m])",
          "legendFormat": "Completion Tokens/s"
        }
      ]
