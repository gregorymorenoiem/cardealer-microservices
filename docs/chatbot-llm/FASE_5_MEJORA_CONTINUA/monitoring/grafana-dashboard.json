      "targets": [
        { "expr": "okla_llm_uptime_seconds", "legendFormat": "Uptime" }
      ]
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [{ "color": "green", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Total Requests",
      "type": "stat",
      "targets": [
        { "expr": "okla_llm_requests_total", "legendFormat": "Total" }
      ]
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 3000 },
              { "color": "red", "value": 8000 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Avg Response Time",
      "type": "stat",
      "targets": [
        { "expr": "okla_llm_avg_response_time_ms", "legendFormat": "Avg" }
      ]
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
